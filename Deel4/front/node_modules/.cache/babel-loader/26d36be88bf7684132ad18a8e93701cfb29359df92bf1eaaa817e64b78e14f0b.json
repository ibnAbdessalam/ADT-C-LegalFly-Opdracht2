{"ast":null,"code":"import { askQuestionApi, analyzeContractApi } from '../services/aiService';\nexport default {\n  name: 'AiService',\n  data() {\n    return {\n      loading: false,\n      question: '',\n      questionResponse: null,\n      contractText: '',\n      contractQuestion: '',\n      contractResponse: null\n    };\n  },\n  methods: {\n    async askQuestion() {\n      if (!this.question.trim()) {\n        alert('Veuillez entrer une question');\n        return;\n      }\n      this.loading = true;\n      this.questionResponse = null;\n      try {\n        this.questionResponse = await askQuestionApi(this.question);\n      } catch (error) {\n        this.questionResponse = {\n          error: error.response?.data || error.message\n        };\n      } finally {\n        this.loading = false;\n      }\n    },\n    async analyzeContract() {\n      if (!this.contractText.trim() || !this.contractQuestion.trim()) {\n        alert('Veuillez remplir le texte du contrat et la question');\n        return;\n      }\n      this.loading = true;\n      this.contractResponse = null;\n      try {\n        this.contractResponse = await analyzeContractApi(this.contractText, this.contractQuestion);\n      } catch (error) {\n        this.contractResponse = {\n          error: error.response?.data || error.message\n        };\n      } finally {\n        this.loading = false;\n      }\n    }\n  }\n};","map":{"version":3,"names":["askQuestionApi","analyzeContractApi","name","data","loading","question","questionResponse","contractText","contractQuestion","contractResponse","methods","askQuestion","trim","alert","error","response","message","analyzeContract"],"sources":["C:\\Github2526\\ADT-C-LegalFly-Opdracht2\\Deel 3\\front\\src\\components\\AiService.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <!-- Ask Question Section -->\r\n    <div class=\"section\">\r\n      <h2>üí¨ Ask Question</h2>\r\n      <div class=\"form-group\">\r\n        <label>Question:</label>\r\n        <textarea v-model=\"question\" placeholder=\"Posez votre question...\"></textarea>\r\n      </div>\r\n      <button @click=\"askQuestion\" :disabled=\"loading\">\r\n        {{ loading ? 'Envoi...' : 'Envoyer' }}\r\n      </button>\r\n\r\n      <div v-if=\"questionResponse\" class=\"response-box\" :class=\"questionResponse.error ? 'error' : 'success'\">\r\n        <strong>R√©ponse:</strong>\r\n        <pre>{{ JSON.stringify(questionResponse, null, 2) }}</pre>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Contract Analysis Section -->\r\n    <div class=\"section\">\r\n      <h2>üìÑ Analyse de Contrat</h2>\r\n      <div class=\"form-group\">\r\n        <label>Texte du contrat:</label>\r\n        <textarea v-model=\"contractText\" placeholder=\"Collez le texte du contrat ici...\"></textarea>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label>Question sur le contrat:</label>\r\n        <input v-model=\"contractQuestion\" placeholder=\"Que voulez-vous savoir sur ce contrat?\" />\r\n      </div>\r\n      <button @click=\"analyzeContract\" :disabled=\"loading\">\r\n        {{ loading ? 'Analyse...' : 'Analyser' }}\r\n      </button>\r\n\r\n      <div v-if=\"contractResponse\" class=\"response-box\" :class=\"contractResponse.error ? 'error' : 'success'\">\r\n        <div v-if=\"!contractResponse.error\">\r\n          <div style=\"margin-bottom: 15px;\">\r\n            <strong>üìù R√©sum√©:</strong>\r\n            <p>{{ contractResponse.summary }}</p>\r\n          </div>\r\n          <div v-if=\"contractResponse.recommendation\">\r\n            <strong>üí° Recommandation:</strong>\r\n            <p>{{ contractResponse.recommendation }}</p>\r\n          </div>\r\n        </div>\r\n        <div v-else>\r\n          <strong>Erreur:</strong>\r\n          <pre>{{ contractResponse.error }}</pre>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { askQuestionApi, analyzeContractApi } from '../services/aiService'\r\n\r\nexport default {\r\n  name: 'AiService',\r\n  data() {\r\n    return {\r\n      loading: false,\r\n      question: '',\r\n      questionResponse: null,\r\n      contractText: '',\r\n      contractQuestion: '',\r\n      contractResponse: null\r\n    }\r\n  },\r\n  methods: {\r\n    async askQuestion() {\r\n      if (!this.question.trim()) {\r\n        alert('Veuillez entrer une question')\r\n        return\r\n      }\r\n\r\n      this.loading = true\r\n      this.questionResponse = null\r\n\r\n      try {\r\n        this.questionResponse = await askQuestionApi(this.question)\r\n      } catch (error) {\r\n        this.questionResponse = {\r\n          error: error.response?.data || error.message\r\n        }\r\n      } finally {\r\n        this.loading = false\r\n      }\r\n    },\r\n\r\n    async analyzeContract() {\r\n      if (!this.contractText.trim() || !this.contractQuestion.trim()) {\r\n        alert('Veuillez remplir le texte du contrat et la question')\r\n        return\r\n      }\r\n\r\n      this.loading = true\r\n      this.contractResponse = null\r\n\r\n      try {\r\n        this.contractResponse = await analyzeContractApi(\r\n            this.contractText,\r\n            this.contractQuestion\r\n        )\r\n      } catch (error) {\r\n        this.contractResponse = {\r\n          error: error.response?.data || error.message\r\n        }\r\n      } finally {\r\n        this.loading = false\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>"],"mappings":"AAuDA,SAASA,cAAc,EAAEC,kBAAiB,QAAS,uBAAsB;AAEzE,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,KAAK;MACdC,QAAQ,EAAE,EAAE;MACZC,gBAAgB,EAAE,IAAI;MACtBC,YAAY,EAAE,EAAE;MAChBC,gBAAgB,EAAE,EAAE;MACpBC,gBAAgB,EAAE;IACpB;EACF,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,WAAWA,CAAA,EAAG;MAClB,IAAI,CAAC,IAAI,CAACN,QAAQ,CAACO,IAAI,CAAC,CAAC,EAAE;QACzBC,KAAK,CAAC,8BAA8B;QACpC;MACF;MAEA,IAAI,CAACT,OAAM,GAAI,IAAG;MAClB,IAAI,CAACE,gBAAe,GAAI,IAAG;MAE3B,IAAI;QACF,IAAI,CAACA,gBAAe,GAAI,MAAMN,cAAc,C